language_revision = 1;
 
information
 
% program = options[:program]
% if program
%   program.information.each do |key, val|
%     unless key == "test_revision"
<%= key %> = <%= val %>
%     end
%   end
% else
-- STOPDIFF
test_revision = "<%= RGen.app.version %>";
-- STARTDIFF
% end
 
end
