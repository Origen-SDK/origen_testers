            measurement.pattern = setupRef(OrigenTesters.patterns.erase_all);
            measurement.specification = setupRef(OrigenTesters.specs.specs.Nominal);
            output = "None";
            testName = "Functional";
        }

        suite erase_all_3 calls ac_tml.AcTest.FunctionalTest {
